{
  "name": "jamstack-api",
  "version": "0.0.3",
  "description": "This is a component demo for Serverless Devs Tool ",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w && npm run doc",
    "publish": "npm run build && s cli platform publish",
    "build": "tsc",
    "test": "jest ./test/index.test.ts",
    "test:cov": "jest --coverage",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix"
  },
  "dependencies": {
    "@alicloud/fc2": "^2.2.2",
    "@serverless-devs/core": "^0.0.*",
    "@serverless-devs/dk-deploy-common": "^0.0.4-alpha.10",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.12.6",
    "f2elint": "^1.1.2",
    "git-cz": "^4.7.6",
    "jest": "^26.4.0",
    "lerna": "^4.0.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "f2elint commit-file-scan",
      "commit-msg": "f2elint commit-msg-scan"
    }
  }
}
